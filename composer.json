{
    "name": "sparkalow/craft-activecampaign-forms",
    "description": "ActiveCampaign form field for CraftCMS",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ActiveCampaign",
        "AC Forms",
        "Field Type",
        "forms"
    ],
    "license": "mit",
    "authors": [
        {
            "name": "Brian Matthews",
            "homepage": "https://slapthink.net"
        }
    ],
    "support": {
        "email": "support@slapthink.net",
        "issues": "https://github.com/sparkalow/craft-activecampaign-forms/issues?state=open",
        "source": "https://github.com/sparkalow/craft-activecampaign-forms",
        "docs": "https://github.com/sparkalow/craft-activecampaign-forms",
        "rss": "https://github.com/sparkalow/craft-activecampaign-forms/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "sparkalow\\activecampaignforms\\": "src/"
        }
    },
    "extra": {
        "handle": "activecampaign-forms",
        "name": "ActiveCampaign Forms",
        "developer": "Brian Matthews",
        "documentationUrl": "https://github.com/sparkalow/craft-activecampaign-forms"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

